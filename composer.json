{
    "name": "amgportalapp/plugins",
    "description": "Core stuff for AMGPortal plugin support.",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": "^9.0",
        "symfony/process": "^6.0",
        "illuminate/console": "^9.0",
        "illuminate/filesystem": "^9.0",
        "illuminate/view": "^9.0",
        "illuminate/auth": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AMGPortal\\Plugins\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AMGPortal\\Plugins\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AMGPortal\\Plugins\\PluginsServiceProvider"
            ]
        }
    }
}
